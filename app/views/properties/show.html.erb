<div class="property-main-photo" style="background-image:url('<%= @property.photo.url %>')">
  <!-- <%= image_tag @property.photo.url %> -->
  <!-- <div class="photo-overlay">

  </div> -->
</div>

<div class="container">
  <div class="row">
    <div class="col-8">
      <p>
        <strong>Name:</strong>
        <%= @property.name %>
      </p>

      <p>
        <strong>Address:</strong>
        <%= @property.address %>
      </p>

      <p>
        <strong>Price:</strong>
        <%= @property.price %>
      </p>

      <p>
        <strong>Rooms:</strong>
        <%= @property.rooms %>
      </p>

      <p>
        <strong>Bathrooms:</strong>
        <%= @property.bathrooms %>
      </p>

      <p>
        <strong>Account:</strong>
        <%= @property.account_id %>
      </p>

      <%= link_to 'Edit Property', edit_property_path(@property), class: 'btn btn-success' %>
    </div>
    <div class="col-4">
      <%= render 'properties/agent_details' %>
    </div>
  </div>
</div>
